{
  "name": "aws-practice",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon",
    "build:prod": "node -r esbuild-runner/register build.js",
    "start:pm2": "npm run build:prod && pm2 start build/index.js --watch build/index.js --name aws-practice --log-date-format 'YYYY-MM-DD HH:mm:ss'",
    "stop:pm2": "pm2 stop aws-practice",
    "check-types": "tsc --noemit",
    "eslint": "eslint '**/*.ts'",
    "lint": "npm run check-types && npm run eslint",
    "lint:fix": "eslint --fix '**/*.ts'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.772.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.2",
    "esbuild-node-tsc": "^2.0.5",
    "esbuild-runner": "^2.2.2",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  }
}
